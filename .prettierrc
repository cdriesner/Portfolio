{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "singleAttributePerLine": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "experimentalTernaries": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
